   "rules": {
      "indent": [
         "off",
         3
      ],
      "linebreak-style": [
         "off",
         "windows"
      ],
      "quotes": [
         "off",
         "double"
      ],
      "semi": [
         "error",
         "always"
      ],
      "no-extra-semi": [
         "warn"
      ],
      "no-undef": [
         "warn"
      ],
      "no-cond-assign": [
         "error"
      ],
      "no-extra-parens": [
         "off"
      ],
      "no-unreachable": [
         "off"
      ],
      "use-isnan": [
         "warn"
      ],
      "accessor-pairs": [
         "warn"
      ],
      "no-fallthrough": [
         "error"
      ],
      "no-multi-spaces": [
         "off"
      ],
      "no-unmodified-loop-condition": [
         "warn"
      ],
      "vars-on-top": [
         "off"
      ],
      "no-unused-vars": [
         "off"
      ],
      "no-irregular-whitespace": [
         "warn"
      ],
      "no-redeclare": [
         "off"
      ],
      "no-control-regex": [
         "off"
      ],
      "no-empty": [
         "off"
      ],
      "no-console": [
         "off"
      ],
      "no-useless-escape": [
         "off"
      ],
      "no-inner-declarations": [
         "off"
      ]
